# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Stage Configuration
STAGE=dev

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# JWT Configuration
JWT_SECRET=your-jwt-secret-key

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key

# GitHub Configuration
GITHUB_TOKEN=your-github-token

# Monitoring Configuration
CLOUDWATCH_LOG_GROUP=/aws/lambda/x-post-ai-generator
CLOUDWATCH_LOG_RETENTION_DAYS=7

# Performance Configuration
LAMBDA_TIMEOUT=30
LAMBDA_MEMORY_SIZE=512

# Security Configuration
CORS_ORIGINS=http://localhost:3010,https://your-domain.com
RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Database Configuration
DATABASE_CONNECTION_POOL_SIZE=5
DATABASE_TIMEOUT_MS=30000

# Testing Configuration
TEST_USER_ID=11111111-1111-1111-1111-111111111111
TEST_DATABASE_URL=postgresql://localhost:5432/test_db
