{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:staging": "NODE_ENV=staging next dev",
    "build": "next build",
    "build:staging": "NODE_ENV=staging next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "start:staging": "NODE_ENV=staging next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "quality": "pnpm run type-check && pnpm run lint && pnpm run format:check && pnpm run test",
    "env:dev": "cp .env.development .env.local",
    "env:staging": "cp .env.staging .env.local", 
    "env:prod": "cp .env.production .env.local"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^1.10.0",
    "@supabase/supabase-js": "^2.50.3",
    "@tanstack/react-query": "^5.0.0",
    "clsx": "^2.0.0",
    "next": "15.3.5",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.0",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
