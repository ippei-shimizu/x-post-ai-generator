{
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--read-only",
        "--project-ref=<your-supabase-project-ref>"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "<your-supabase-personal-access-token>"
      }
    }
  }
}